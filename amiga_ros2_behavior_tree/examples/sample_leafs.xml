<root BTCPP_format="4" schema_location="schemas/amiga_btcpp.xsd">
    <Mission>go to the first tree and sample leaves</Mission>
    <BehaviorTree ID="MainTree">
        <Sequence>
            <MoveToGPSLocation name="MoveToTree1" action_name="follow_gps_waypoints" object_angle="{tree_angle}" latitude="37.26643908" longitude="-120.42079453"/>
            <SampleLeaf name="SampleLeavesTree1" action_name="navigate_via_lidar" tree_angle="{tree_angle}"/>
        </Sequence>
    </BehaviorTree>
</root>
