<root BTCPP_format="4">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <MoveToGPSLocation action_name="follow_gps_waypoints" latitude="37.4219983" longitude="-122.084"/>
      <DetectObject object="tree" detected="{result}"/>

      <!-- Branch: if object detected, else... -->
      <Fallback>
        <Sequence>
          <BoolCondition value="{result}" expected="true"/>
          <MoveToGPSLocation action_name="follow_gps_waypoints" latitude="37.55" longitude="-122.084"/>
        </Sequence>

        <Sequence>
          <BoolCondition value="{result}" expected="false"/>
          <MoveToGPSLocation action_name="follow_gps_waypoints" latitude="37.66" longitude="-122.084"/>
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
